+                <span>Very satisfied</span>
+              </div>
+            </div>
+            <div className="flex gap-2 justify-end">
+              <Button
+                variant="outline"
+                size="sm"
+                onClick={() => setShowSatisfaction(false)}
+              >
+                Cancel
+              </Button>
+              <Button size="sm" onClick={confirmComplete}>
+                Complete
+              </Button>
+            </div>
+          </div>
+        </div>
