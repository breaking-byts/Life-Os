+  lastUpdatedAt: number
+}
+
+export type PersistedPomodoroState = PersistedPomodoroStateV1
+
+export interface PomodoroRuntimeReset {
+  mode: PomodoroTimerMode
+  state: PomodoroTimerState
+  timeLeft: number
+  cycles: number
