+    let row = sqlx::query_as::<_, Workout>("SELECT id, user_id, name, duration_minutes, notes, logged_at FROM workouts WHERE id = ?")
+        .bind(id)
+        .fetch_one(pool)
+        .await
+        .map_err(|e| e.to_string())?;
+    Ok(row)
+}
+
+#[tauri::command]
+pub async fn delete_workout(state: State<'_, DbState>, id: i64) -> Result<bool, String> {
+    let pool = &state.0;
+    sqlx::query("DELETE FROM workouts WHERE id = ?")
+        .bind(id)
+        .execute(pool)
+        .await
+        .map_err(|e| e.to_string())?;
+    Ok(true)
+}
+
+#[tauri::command]
+pub async fn update_workout(
+    state: State<'_, DbState>,
+    id: i64,
+    data: WorkoutInput,
+) -> Result<Workout, String> {
+    let pool = &state.0;
+    let rec = sqlx::query_as::<_, Workout>(
+        "UPDATE workouts SET
+            name = COALESCE(?, name),
+            duration_minutes = COALESCE(?, duration_minutes),
+            notes = COALESCE(?, notes),
+            logged_at = COALESCE(?, logged_at)
+         WHERE id = ?
+         RETURNING id, user_id, name, duration_minutes, notes, logged_at"
+    )
+    .bind(&data.name)
+    .bind(data.duration_minutes)
+    .bind(&data.notes)
+    .bind(&data.logged_at)
+    .bind(id)
+    .fetch_one(pool)
+    .await
+    .map_err(|e| e.to_string())?;
+    Ok(rec)
+}
+
