+use serde::{Deserialize, Serialize};
+use sqlx::FromRow;
+
+#[derive(Debug, Serialize, Deserialize, FromRow, Clone)]
+pub struct Assignment {
+    pub id: i64,
+    pub course_id: i64,
+    pub title: String,
+    pub description: Option<String>,
