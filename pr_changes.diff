+            "good mood"
+        } else if self.mood_level > 0.4 {
+            "neutral mood"
+        } else {
+            "low mood"
+        };
+
+        let time_desc = if self.hour_of_day < 0.5 {
+            "morning"
+        } else if self.hour_of_day < 0.75 {
+            "afternoon"
+        } else {
+            "evening"
+        };
+
+        let workload_desc = if self.active_assignments > 0.3 {
+            "high workload"
+        } else if self.active_assignments > 0.1 {
+            "moderate workload"
+        } else {
+            "light workload"
+        };
+
+        format!(
+            "{}, {}, {} {}, {}",
+            energy_desc,
+            mood_desc,
+            if self.is_weekend > 0.5 {
+                "weekend"
+            } else {
+                "weekday"
+            },
+            time_desc,
+            workload_desc
+        )
+    }
