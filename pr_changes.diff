+                            {course.current_grade.toFixed(1)}%
+                            {course.target_grade && (
+                              <span className="text-muted-foreground">
+                                {' '}
+                                / {course.target_grade}%
+                              </span>
+                            )}
+                          </span>
+                        )}
+                      <span className="text-muted-foreground/60">
