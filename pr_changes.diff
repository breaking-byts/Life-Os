+CREATE TABLE IF NOT EXISTS practice_logs (
+  id INTEGER PRIMARY KEY AUTOINCREMENT,
+  skill_id INTEGER NOT NULL,
+  duration_minutes INTEGER NOT NULL,
+  notes TEXT,
+  logged_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
