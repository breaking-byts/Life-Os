+                <div
+                  key={exercise.id}
+                  className="flex items-center justify-between text-sm p-2 rounded-md bg-background"
+                >
+                  <span className="font-medium">{exercise.exercise_name}</span>
+                  <div className="flex items-center gap-2 text-muted-foreground">
