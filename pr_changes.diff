+        </Button>
+      </div>
+
+      {/* Quick Stats Row */}
+      <div className="grid gap-4 md:grid-cols-4">
+        <Card>
+          <CardContent className="pt-4">
+            <div className="flex items-center gap-2">
+              <div className="bg-blue-500/10 rounded-md p-2">
+                <Clock className="h-4 w-4 text-blue-500" />
