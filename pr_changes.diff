+              <p className="text-muted-foreground text-xs font-medium">
+                {stat.label}
+              </p>
+              <p className="text-lg font-semibold">{stat.value}</p>
+            </div>
+          </CardContent>
+        </Card>
+      ))}
+    </div>
+  )
