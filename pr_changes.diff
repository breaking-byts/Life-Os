+    pub default_weight: Option<f64>,
+    pub order_index: Option<i64>,
+}
+
+#[tauri::command]
+pub async fn get_workout_templates(state: State<'_, DbState>) -> Result<Vec<WorkoutTemplate>, String> {
+    let pool = &state.0;
